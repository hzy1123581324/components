/* components/input-list/index.wxss */
.input_items{
    display: flex;
    background-color:#fff;
    height: 80rpx;
    margin-bottom: 2rpx;
    align-items: center;
    align-content: center;
    justify-content: flex-start;
    padding-left: 30rpx;
    padding-right: 30rpx;
}
.input_label{
    width: 5em;
    flex-shrink: 0;
    display: flex;
    align-items: center;
    align-content: center;
    justify-content: flex-start;
    height: 100%;
}
.input_init{
    flex-grow: 1;
    margin-left: 24rpx;
    height: 100%;
    font-size: inherit;
}
.input_slot{
    flex-shrink: 0;
}




